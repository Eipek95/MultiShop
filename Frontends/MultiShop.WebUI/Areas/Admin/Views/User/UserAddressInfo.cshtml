@model GetCargoCustomerByIdDto
@{
    ViewData["Title"] = "UserAddressInfo";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}

<div class="container">
    <div class="row">
        <h3 class="mt-3">Üye Adres Bilgisi</h3>
        <hr />
        <div class="col-md-12 justify-content-between">
            <div class="alert alert-warning">Üye Adı: @Model.Name</div>
            <div class="alert alert-info">Üye Soyadı: @Model.Surname</div>
            <div class="alert alert-warning">Üye Mail: @Model.Email</div>
            <div class="alert alert-secondary">Üye Telefon: @Model.Phone</div>
            <div class="alert alert-warning">Üye İlçe: @Model.District</div>
            <div class="alert alert-warning">Üye Şehir: @Model.City</div>
            <div class="alert alert-warning">Üye Adres: @Model.Address</div>
        </div>
    </div>
</div>

